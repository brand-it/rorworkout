<%= link_to "New", new_doctor_path %>
<table>
	<thead>
		<tr>
			<th>
				Doctor Name
			</th>
			<th>Address</th>
			<th>Zip</th>
			<th>Years of Experience</th>
			<th>Created By</th>
		</tr>
		</thead>
		<tbody>
			<% @doctors.each do |doctor| %>
				<tr>
					<td><%= link_to doctor.name, doctor_path(doctor) %></td>
					<td><%= doctor.address%></td>
					<td><%= doctor.zip %></td>
					<td><%= doctor.years_of_experience %></td>
					<td><%= doctor.user.full_name %></td>
				</tr>
				
			<% end %>
		</tbody>
</table>